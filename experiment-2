create database student;
use student;
create table student2(id int not null,name char(20),course char(20),primary key(id));

create table course1(course char(20) references student1(course),course_no int,course_fees int);

CREATE TABLE Persons (ID int NOT NULL,LastName varchar(255) NOT NULL,FirstName varchar(255),Age int,UNIQUE (ID));

CREATE TABLE Persons1(ID int NOT NULL,LastName varchar(255) NOT NULL,FirstName varchar(255),Age int CHECK (Age>=18));

alter table Persons1 add check (Age>=18);


